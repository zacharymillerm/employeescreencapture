	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Hardcodet.Wpf.TaskbarNotification" Version="1.0.5" />
	</ItemGroup>

	<ItemGroup>
	  <Reference Include="System.Net.Http" />
	  <Reference Include="System.Windows.Forms" />
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="Views\eye-scan.png" />
	</ItemGroup>

</Project>
